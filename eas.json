{
  "cli": {
    "version": ">= 9.0.0",
    "appVersionSource": "local"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_API_URL": "https://api-de-produccion.tu-dominio.com" 
      }
    },
    "production": {
      "autoIncrement": true,
      "env": {
        "EXPO_PUBLIC_API_URL": "https://api-de-produccion.tu-dominio.com"
      },
      "android": {
        "buildType": "aab"
      },
      "ios": {
        "image": "latest" 
      }
    }
  },
  "submit": {
    "production": {}
  }
}